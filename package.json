{
  "name": "WebpackAngularBabel",
  "version": "1.0.0",
  "description": "An angular app to explore webpack and babeljs with ondemand angular module loading",
  "main": "gulpfile.js",
  "scripts": {
    "build-route": "node route-generator.js",
    "build-app": "node node_modules/webpack/bin/webpack.js --colors --progress --config webpack.config.js",
    "test": "karma start karma.config.js",
    "clean": "rimraf dist coverage plugins/**/dist plugins/**/coverage plugins/**/node_modules",
    "doc": "rimraf doc && node node_modules/jsdoc/jsdoc.js -c conf.json",
    "server": "node start.js",
    "start": "npm run build-route && npm run build-app | npm run server"
  },
  "keywords": [
    "webpack",
    "gulp",
    "babeljs",
    "angularjs",
    "mocha",
    "karma"
  ],
  "engines": {
    "node": "4.1.1"
  },
  "author": "Jaganathan B",
  "license": "MIT",
  "dependencies": {},
